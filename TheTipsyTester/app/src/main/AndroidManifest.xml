<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thetipsytester.thetipsytester">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tipsy_tester"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".testSelectActivity"
            android:label="@string/title_activity_test_select"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thetipsytester.thetipsytester.MainActivity" />
        </activity>
        <activity
            android:name=".baselineActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".bacCalculatorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".scoresActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".settingsActivity" />
        <activity android:name=".newUserActivity" />
        <activity android:name=".singleTestSelectActivity" />
        <activity
            android:name=".balanceTest"
            android:screenOrientation="portrait" />
        <activity
            android:name=".patternTest"
            android:screenOrientation="portrait" />
        <activity
            android:name=".typingTest"
            android:screenOrientation="portrait" />
        <activity
            android:name=".schwack_a_moleaa"
            android:screenOrientation="portrait" />
        <activity
            android:name=".scorereportActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".userSelectActivity" />
        <activity android:name=".comet_smash" />
        <activity
            android:name=".estimatedBAC"
            android:label="@string/title_activity_estimated_bac"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
